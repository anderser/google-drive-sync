{
  "name": "google-drive-sync",
  "version": "1.0.9",
  "description": "Share your Google Drive files with this daemon to have them saved to disk as JSON.",
  "bin": {
    "google-drive-sync": "lib/cli.js",
    "google-drive-sync-convert": "lib/convert-cli.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha --compilers js:babel-core/register test/*"
  },
  "repository": {
    "type": "git",
    "url": "git@githubcom:jarib/google-drive-sync.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "author": "Jari Bakken <jari.bakken@gmail.com>",
  "license": "BSD-3-Clause",
  "keywords": [
    "google",
    "drive",
    "google-drive",
    "archieml",
    "json",
    "docs",
    "sheets",
    "spreadsheets",
    "excel",
    "xlsx"
  ],
  "dependencies": {
    "archieml": "^0.4.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "bluebird": "^3.3.4",
    "daemon": "^1.1.0",
    "debug": "^2.2.0",
    "fs-promise": "^0.3.1",
    "googleapis": "^5.2.1",
    "html-entities": "^1.2.0",
    "htmlparser2": "^3.9.0",
    "lodash.zip": "^4.0.0",
    "moment": "^2.12.0",
    "stream-buffers": "^3.0.0",
    "timestring": "^1.1.1",
    "underscore": "^1.8.3",
    "xlsx": "^0.8.0",
    "yargs": "^3.32.0"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "node-fetch": "^1.4.0"
  }
}
